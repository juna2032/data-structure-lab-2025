t[index]\n","        position = index\n","\n","        while position > 0 and a_list[position - 1] > current_value:\n","            a_list[position] = a_list[position - 1]\n","            position = position - 1\n","\n","        a_list[position] = current_value\n","\n","\n","numbers = [7, 4, 5, 2, 9, 1]\n","print(\"정렬 전:\", numbers)\n","\n","insertion_sort(numbers)\n","\n","print(\"정렬 후:\", numbers)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"3chTyiKNAJrw","executionInfo":{"status":"ok","timestamp":1747616407968,"user_tz":-540,"elapsed":45,"user":{"displayName":"이준하","userId":"09528861880823512878"}},"outputId":"136af689-1807-417c-be32-eec04b8e05a2"},"execution_count":4,"outputs":[{"output_type":"stream","name":"stdout","text":["정렬 전: [7, 4, 5, 2, 9, 1]\n","정렬 후: [1, 2, 4, 5, 7, 9]\n"]}]}]}